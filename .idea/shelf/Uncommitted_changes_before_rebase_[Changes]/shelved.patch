<+>@import url('https://fonts.googleapis.com/css2?family=Pixelify+Sans:wght@400..700&display=swap');\n\nbody, html {\n    height: auto;\n    width: auto;\n    margin: 0;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    flex-direction: column;\n    gap: 20px;\n    background-color: #333;\n    padding: 20px;\n    font-family: 'Pixelify Sans', sans-serif;\n    font-weight: bold;\n    overflow: hidden;\n    image-rendering: pixelated;\n}\n\nh1 {\n    font-size: 128px;\n    color: red;\n    margin: 0;\n}\n\n/* Screen Styles */\n\n.screen {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    display: none;\n}\n\n.screen.active {\n    display: flex;\n}\n\n#gameScreen {\n\n}\n\n/* Button Styles */\n\n.btn {\n    background-color: red;\n    border: 4px solid yellow;\n    color: yellow;\n    font-family: 'Pixelify Sans', sans-serif;\n    font-size: 36px;\n    padding: 15px 20px;\n    text-transform: uppercase;\n    cursor: pointer;\n    text-shadow: 2px 2px 0px #000;\n    box-shadow: 4px 4px 0px rgba(0, 0, 0, 0.6);\n    transition: all 0.1s;\n    width: 100%;\n    margin-bottom: 10px;\n    text-align: center;\n}\n\n.btn:hover {\n    background-color: yellow;\n    color: red;\n    transform: translate(-2px, -2px);\n    box-shadow: 6px 6px 0px rgba(0, 0, 0, 0.6);\n}\n\n/* Title Styles */\n\n.gameTitle {\n    color: red;\n    margin-bottom: 50px;\n    text-shadow: 4px 4px 0px yellow;\n    text-align: center;\n}\n\n.screenTitle {\n    font-size: 64px;\n    color: yellow;\n    margin-bottom: 30px;\n    text-shadow: 4px 4px 0px red;\n    text-align: center;\n}\n\n/* Layout Styles */\n\n.menu {\n    width: 350px;\n    max-width: 90%;\n    display: flex;\n    flex-direction: column;\n    gap: 15px;\n}\n\n.panel {\n    padding: 20px;\n    width: auto;\n    max-width: 90%;\n    margin-bottom: 40px;\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    align-items: stretch;\n    gap: 100px;\n}\n\n.settings {\n    display: flex;\n    flex-direction: column;\n    gap: 15px;\n    border: 4px solid yellow;\n    box-shadow: 4px 4px 0px rgba(0, 0, 0, 0.6);\n    padding: 40px;\n}\n\n.statsPanel p {\n    font-size: 36px;\n    margin: 10px 0;\n    display: flex;\n    justify-content: space-between;\n    width: 250px;\n    color: yellow;\n}\n\n.stat-val {\n    color: yellow;\n}\n\n.flex-row-gap {\n    display: flex;\n    gap: 15px;\n    max-width: 90%;\n    width: auto;\n}\n\n.flex-row-gap .btn {\n    margin-bottom: 0; /* Margin im Flex-Container entfernen */\n}\n\n.overlay-dark {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n\n    background-color: rgba(0, 0, 0, 0.7);\n    backdrop-filter: blur(5px);\n\n\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n\n    z-index: 10;\n\n    display: none;\n}\n\n/*HUD Styles*/\n/* --- 8-BIT HUD TOP PANEL STYLES (FLEX VERSION) --- */\n\n.hud-top-panel-flex {\n    /* Wir nutzen jetzt Flexbox */\n    display: flex;\n    align-items: center; /* Vertikale Zentrierung */\n    /* justify-content ist hier eigentlich egal, da die Kinder flex: 1 haben */\n\n    /* Positionierung ganz oben (gleich wie vorher) */\n    width: 100%;\n    position: absolute;\n    top: 0;\n    left: 0;\n    z-index: 100;\n\n    /* 8-Bit Panel Look (gleich wie vorher) */\n    background-color: brown;\n    border-bottom: 4px solid #7c5e35;\n    box-shadow: 0px 4px 0px rgba(0, 0, 0, 0.4);\n\n    /* Typografie Basis (gleich wie vorher) */\n    font-family: 'Pixelify Sans', sans-serif;\n    font-size: 28px;\n    color: #ffffff;\n    text-transform: uppercase;\n    /* Verhindert, dass die Zeit/XP umbrechen, wenn es eng wird */\n    white-space: nowrap;\n}\n\n/* --- Die Magie der perfekten Zentrierung --- */\n\n/* Alle 3 Sektionen bekommen exakt den gleichen Platzanteil */\n.hud-section {\n    flex: 1;\n    display: flex; /* Damit wir den Inhalt darin ausrichten können */\n}\n\n/* Inhalte in den Sektionen ausrichten */\n.section-left {\n    justify-content: center;\n}\n\n.section-center {\n    justify-content: center;\n}\n\n.section-right {\n    justify-content: center\n}\n\n\n/* --- Styling der eigentlichen Inhalte (fast gleich wie vorher) --- */\n\n.hud-title {\n    color: yellow;\n    font-size: 36px;\n    text-shadow: 2px 2px 0px #000;\n}\n\n.hud-xp {\n    color: yellow;\n}\n\n.hud-item span {\n    color: #ffffff;\n}\n/* Ende HUD Styles */
